<div id="<%= dom_id home %>">
  <% if action_name != "show" %>
    <%= link_to "Show this home", home, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Edit this home", edit_home_path(home), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
